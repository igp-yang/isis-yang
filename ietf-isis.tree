      |     +--ro level?                level-number
      |     +--ro lsp
      |     |  +--ro lsp?        lsp-id
      |     |  +--ro sequence?   uint32
      |     +--ro received-timestamp?   yang:timestamp
      +--ro database
      |  +--ro level-db* [level]
