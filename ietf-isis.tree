      |     +--ro circuit-id?            circuit-id
      |     +--ro admin-state?           admin-state
      |     +--ro oper-state?            oper-state
      |     +--ro interface-type?        interface-type
      |     +--ro level?                 level
      |     +--ro passive?               empty
      |     +--ro three-way-handshake?   empty
      +--ro adjacencies
